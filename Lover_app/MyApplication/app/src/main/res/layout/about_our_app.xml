            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:layout_marginLeft="10sp"
            android:layout_marginTop="75dp"
            android:text="后端技术支持"
            android:gravity="left"
            android:layout_toEndOf="@+id/image23"
            android:layout_below="@+id/tex3t"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true"/>
    </RelativeLayout>
    <RelativeLayout android:orientation="vertical"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="5sp">
        <com.example.myapplication.personal.ZQImageViewRoundOval
            android:layout_width="70sp"
            android:layout_height="70dp"
            android:layout_marginTop="15sp"
            android:layout_marginLeft="20sp"
            android:id="@+id/image24"
            android:src="@drawable/o_hh"
            />
        <TextView android:textColor="#005A78"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:text="胡竟竟"
            android:layout_marginLeft="10sp"
            android:layout_marginTop="10dp"
            android:textSize="15dp"
            android:layout_toRightOf="@id/image24"
            android:gravity="left"
            />
        <TextView android:textColor="#005A78"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:text="电子科学与技术专业"
            android:layout_marginLeft="10sp"
            android:layout_marginTop="32dp"
            android:textSize="15dp"
            android:layout_toRightOf="@id/image24"
            android:gravity="left"
            />
        <TextView android:textColor="#005A78"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:text="QQ："
            android:layout_marginLeft="10sp"
            android:layout_marginTop="55dp"
            android:textSize="15dp"
            android:layout_toRightOf="@id/image24"
            android:gravity="left"
            />
        <TextView android:textColor="#005A78"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:layout_marginLeft="10sp"
            android:layout_marginTop="75dp"
            android:text="官方网站负责人员"
            android:gravity="left"
            android:layout_toEndOf="@+id/image24"
        />
    </RelativeLayout>
    <RelativeLayout android:orientation="vertical"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="5sp"
        android:layout_below="@+id/relativeLayout">
        <com.example.myapplication.personal.ZQImageViewRoundOval
            android:layout_width="70sp"
            android:layout_height="70dp"
            android:layout_marginTop="15sp"
            android:layout_marginLeft="20sp"
            android:id="@+id/image2"
            android:src="@drawable/l_beijinggereng"
